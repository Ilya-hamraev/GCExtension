{
  "manifest_version": 3,
  "name": "my bookmarks",
  "version": "1.0.0",
  "author": "Ilya Khamraev",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "My bookmarks"
  },
  "permissions ": ["storage"]
}
